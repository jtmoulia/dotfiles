#!/usr/bin/env bash
for key in ~/.ssh/{id_rsa,id_rsa_gitlab,hippo,research-bastion.pem,cesium-test.pem}; do
    ssh-add "$key"
done

pass-show() {
    pass show "$1" | head -n1
}

